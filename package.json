{
  "name": "eleventy-structured-content-starter",
  "version": "0.0.0",
  "description": "A component-based, structured content starter for Eleventy",
  "type": "module",
  "keywords": [
    "eleventy",
    "11ty",
    "starter",
    "components",
    "structured-content",
    "static-site-generator"
  ],
  "author": "werner@glinka.co",
  "license": "MIT",
  "scripts": {
