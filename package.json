  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "eleventy-plugin-components-bundler": "^0.0.1",
    "eleventy-plugin-normalized-collections": "^0.0.3",
    "marked": "^17.0.1",
    "picomatch": "^4.0.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
